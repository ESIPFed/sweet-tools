  <rdf:Description rdf:about="http://gcmdservices.gsfc.nasa.gov/kms/concept/47b1f007-ab9b-41ca-abdf-fcaa2b5b83dc">
    <skos:broader rdf:resource="http://gcmdservices.gsfc.nasa.gov/kms/concept/4190ebdc-e89a-4f3f-bc74-b2af4c68d009"/>
    <skos:altLabel xml:lang="en">TIROS-N OPERATIONAL VERTICAL SOUNDER</skos:altLabel>
    <skos:prefLabel xml:lang="en">TIROS-N</skos:prefLabel>
    <skos:inScheme rdf:resource=""/>
