        skos:broader     <http://gcmdservices.gsfc.nasa.gov/kms/concept/ebfff02c-2e5a-476e-aafb-c00167bf2daa> ;
        skos:definition  "The Hobo XT Air Tempetature Logger is a match-book-size,  single-channel, 1800\ndata point capacity data logger for  temperature. The HOBO-Temp has its\ntemperature sensor built into  the casing. This logger provides a temperature\nrange from -40  deg.F to 253 deg. F and also has an external temperature\nsensor. \n\nView image at \nhttp://arch.ced.berkeley.edu/vitalsigns/equip/tools/xhoboxt.jpg \n\n[Source: University of California, Berkeley] \n"@en ;
        skos:inScheme    <> ;
        skos:prefLabel   "HATDL"@en .

<http://gcmdservices.gsfc.nasa.gov/kms/concept/c8fe757b-b530-4d67-a553-e4903f4430a5>
