        skos:broader     <http://gcmdservices.gsfc.nasa.gov/kms/concept/992ffede-af18-49a9-8acb-2cd333860efe> ;
        skos:definition  "\nThe Stratospheric Infrared Interferometer Spectrometer (SIRIS)\nwas first carried on Nimbus 3, to obtain temperature soundings\nof the atmosphere. It had eight detectors, Fastie-Ebert optics,\nand a 12-degree field of view.\n\nAdditional information available at\n\"http://euromet.meteo.fr/demos/courses/glossary/satellit.htm\"\n\n[Summary provided by EuroMET]\n"@en ;
        skos:inScheme    <> ;
        skos:prefLabel   "SIRIS"@en .

