<http://gcmdservices.gsfc.nasa.gov/kms/concept/b879e422-8370-4f09-a52d-8ebfbdf37493>
        a                skos:Concept ;
        skos:broader     <http://gcmdservices.gsfc.nasa.gov/kms/concept/eceec9a6-1311-4028-8433-1f6d21b62d4d> ;
